// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model quotes {
  id        Int      @default(autoincrement()) @id
  quote     String
  author    String
}
model users {
  id          String       @id @default(uuid())
  email       String       @unique
  name        String
  password    String
}